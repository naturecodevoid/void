{
    "name": "void-sveltelib",
    "version": "0.0.4",
    "license": "MIT",
    "author": {
        "name": "naturecodevoid",
        "url": "https://naturecodevoid.dev/"
    },
    "repository": "github:naturecodevoid/void-sveltelib",
    "bugs": {
        "url": "https://github.com/naturecodevoid/void-sveltelib/issues"
    },
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "test": "vitest",
        "build": "bun run format && bun run check && vitest --run && svelte-package",
        "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "format": "prettier --plugin @trivago/prettier-plugin-sort-imports --plugin prettier-plugin-svelte --config ~/.prettierrc.json ./src --write"
    },
    "peerDependencies": {
        "svelte": "^3.54.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/kit": "^1.5.0",
        "@sveltejs/package": "^1.0.0",
        "@trivago/prettier-plugin-sort-imports": "^4.0.0",
        "prettier": "^2.8.4",
        "prettier-plugin-svelte": "^2.9.0",
        "sass": "^1.58.0",
        "svelte": "^3.54.0",
        "svelte-check": "^3.0.1",
        "svelte-preprocess": "^5.0.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vitest": "^0.28.4"
    }
}
